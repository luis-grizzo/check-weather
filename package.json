{
  "name": "check-weather",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate --no-engine",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate --no-engine",
    "prisma:dev:migrate": "npx prisma migrate dev --name",
    "prisma:dev:db": "npx prisma dev --name checkweather",
    "prisma:dev:remove:db": "npx prisma dev rm checkweather",
    "prisma:remote:migrate": "npx prisma migrate deploy",
    "prisma:remote:reset": "npx prisma migrate reset --force --skip-generate --skip-seed"
  },
  "dependencies": {
    "@google/genai": "^1.29.1",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "pexels": "^1.4.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
